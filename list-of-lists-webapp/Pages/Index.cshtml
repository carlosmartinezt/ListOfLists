@page
@model IndexModel
@using Microsoft.AspNetCore.Authentication
@{
    ViewData["Title"] = "Home page";
}

<h1>Welcome</h1>
<ul>
    @foreach (var item in Model.DataTypes)
    {
        <li>@item.SystemType</li>
    }

    <h2>Claims</h2>

    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>

    <h2>Properties</h2>

    <dl>
        @foreach (var prop in (await HttpContext.AuthenticateAsync()).Properties.Items)
        {
            <dt>@prop.Key</dt>
            <dd>@prop.Value</dd>
        }
    </dl>
</ul>